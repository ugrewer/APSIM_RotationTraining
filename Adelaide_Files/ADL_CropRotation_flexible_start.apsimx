                "        public double MonthlyHarvestedWt { get; set; }",
                "",
                "        [EventSubscribe(\"StartOfMonth\")]",
                "        private void DoStartOfMonth(object sender, EventArgs e)",
                "        {",
                "            MonthlyHarvestedWt = 0;",
                "        }",
                "    }",
                "}",
